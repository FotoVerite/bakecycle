{
  "name": "bakecycle",
  "version": "1.0.0",
  "repository": "https://github.com/wizarddevelopment/bakecycle",
  "description": "Painless Bakery Operations",
  "homepage": "https://www.bakecycle.com",
  "engines": {
    "node": "4.4.x",
    "npm": "^3.x.x"
  },
  "private": true,
  "scripts": {
    "test": "npm run eslint && npm run unit-test",
    "unit-test": "mocha --compilers js:babel-register app/assets/javascripts/test/*-test.js",
    "eslint": "eslint --ignore-pattern 'vendor/' app/assets/javascripts/ --ext .jsx,.js",
    "discify": "browserify --full-paths -t [ babelify ] --extension=\".jsx\" app/assets/javascripts/app.js | discify --open"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "backbone": "^1.2.1",
    "browserify": "^13.0.1",
    "browserify-incremental": "^3.1.1",
    "classlist-polyfill": "^1.0.2",
    "core-js": "^2.4.0",
    "google-map-react": "^0.4.1",
    "lodash.differenceby": "^4.6.0",
    "lodash.includes": "^4.3.0",
    "lodash.isfinite": "^3.3.1",
    "lodash.mapvalues": "^4.4.0",
    "lodash.uniqueid": "^4.0.0",
    "lodash.without": "^4.2.0",
    "moment": "^2.11.2",
    "react": "^0.14.7",
    "react-datepicker": "^0.23.2",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "react-select": "^1.0.0-beta13",
    "redux": "^3.5.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "disc": "^1.3.2",
    "eslint": "^2.4.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.1",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.5.3",
    "sinon": "1.17.6"
  }
}
