<div class="fields" ng-show="true">
  <div class="row">
   <div class="small-4 columns">
      <%= f.input :inclusionable_id_type, collection: @inclusionable, label: "Ingredient", include_blank: "--None--", as: :select%>
   </div>
   <div class="small-4 columns">
      <%= f.input :bakers_percentage, as: :string, label: "Baker's %", as: :string %>
   </div>
   <div class="small-4 columns">
      <%= f.input :_destroy, as: :hidden, wrapper: false %>
      <%= link_to 'Remove', '', :'ng-click' => "remove($event)" %>
   </div>
  </div>
</div>
