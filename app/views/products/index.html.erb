<h1>Products</h1>
<p><%= link_to 'Add New Product', new_product_path %></p>
<table>
  <tr>
    <th>Name</th>
    <th>Action</th>
  </tr>

  <% @products.each do |product| %>
    <tr>
      <td><%= link_to product.name, edit_product_path(product) %></td>
      <td><%= link_to 'Edit', edit_product_path(product) %></td>
    </tr>
  <% end %>
</table>
