<h1>Clients</h1>
<p><%= link_to 'Add New Client', new_client_path %></p>
<table>
  <tr>
    <th>Name</th>
    <th>Action</th>
  </tr>

  <% @clients.each do |client| %>
    <tr>
      <td><%= link_to client.name, client_path(client) %></td>
      <td><%= link_to 'Edit', edit_client_path(client) %></td>
    </tr>
  <% end %>
</table>
