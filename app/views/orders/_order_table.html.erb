<table class="responsive-table order-index-table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Start Date</th>
      <th scope="col">End Date</th>
      <th scope="col">Client</th>
      <th scope="col">Type</th>
      <th scope="col">Route</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "order", collection: OrderDecorator.decorate_collection(orders), locals: {missing_shipment_ids: missing_shipment_ids } %>
  </tbody>
</table>

<%= will_paginate @orders %>
